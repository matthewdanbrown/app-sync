{
  "name": "app-sync",
  "version": "0.0.2",
  "description": "Runs one or more node-apps keeping them in sync with Github.",
  "main": "./index.js",
  "scripts": {
    "start": "pm2 start -x --no-daemon pm2.json",
    "watch": "nodemon ./example -e js,jsx --ignore .build",
    "watch-env": "nodemon ./lib/load-env -e js,jsx --ignore .build",
    "test": "./node_modules/mocha/bin/mocha --recursive --compilers js:babel/register",
    "tdd": "./node_modules/mocha/bin/mocha --recursive --compilers js:babel/register --watch",
    "prepublish": "gulp build",
    "dbuild": "sh ./sh/docker-build.sh",
    "drun": "sh ./sh/test-docker-run.sh",
    "dstop": "docker stop app-sync",
    "shenv": "sh ./sh/test-start-env.sh"
  },
  "dependencies": {
    "bluebird": "^3.0.5",
    "express": "^4.13.3",
    "file-system-github": "1.1.0",
    "fs-extra": "^0.26.2",
    "http-proxy": "^1.12.0",
    "minimist": "^1.2.0",
    "ramda": "^0.18.0",
    "rest-middleware": "^1.0.23",
    "semver": "^5.0.3",
    "shelljs": "^0.5.3"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-core": "^5.6.18",
    "babel-eslint": "^3.1.23",
    "chai": "^2.2.0",
    "eslint": "^0.24.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.2.0",
    "gulp-eslint": "^0.15.0",
    "gulp-plumber": "^1.0.1",
    "mocha": "^2.2.1",
    "sinon": "^1.17.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philcockfield/app-sync"
  },
  "keywords": [
    "sync,github,pm2,gateway"
  ],
  "author": {
    "name": "Phil Cockfield",
    "email": "phil@cockfield.net",
    "url": "https://github.com/philcockfield"
  },
  "homepage": "https://github.com/philcockfield/app-sync",
  "license": "MIT"
}
